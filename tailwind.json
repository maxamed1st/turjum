{
	"mt-auto": {
		"style": {
			"marginTop": "auto"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"w-['100%']": {
		"style": {
			"width": "'100%'"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-start": {
		"style": {
			"justifyContent": "flex-start"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded": {
		"style": {
			"borderTopLeftRadius": 4,
			"borderTopRightRadius": 4,
			"borderBottomRightRadius": 4,
			"borderBottomLeftRadius": 4
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-blue-500": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(59 130 246 / var(--tw-bg-opacity))"
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-3": {
		"style": {
			"paddingTop": 12,
			"paddingBottom": 12
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-xs": {
		"style": {
			"fontSize": 12,
			"lineHeight": 16
		}
	},
	"text-blue-600": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(37 99 235 / var(--tw-text-opacity))"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	}
}